{
  "name": "library-manager",
  "version": "0.0.0",
  "description": "A simple library manager app that add, updates and deletes books",
  "main": "index.js",
  "scripts": {
    "server": "node app.js",
    "proxy": "delay 3 && browser-sync start --proxy localhost:3000 --files app.js --watch app.js",
    "open": "start http://localhost:3000/",
    "start": "npm-run-all --parallel server proxy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Michael Braga",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "csso": "^3.5.1",
    "delay-cli": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0"
  },
  "dependencies": {
    "express": "^4.16.4",
    "pug": "^2.0.3",
    "ramda": "^0.26.1",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "sqlite3": "^4.0.6"
  }
}
